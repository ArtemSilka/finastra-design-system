{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "test": {
      "projectType": "application",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "root": "apps/angular-test-app",
      "sourceRoot": "apps/angular-test-app/src",
      "prefix": "ffdc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/angular-test-app",
            "index": "apps/angular-test-app/src/index.html",
            "main": "apps/angular-test-app/src/main.ts",
            "polyfills": "apps/angular-test-app/src/polyfills.ts",
            "tsConfig": "apps/angular-test-app/tsconfig.app.json",
            "assets": [
              "apps/angular-test-app/src/favicon.ico",
              "apps/angular-test-app/src/assets"
            ],
            "styles": [
              "apps/angular-test-app/src/styles.scss",
              "apps/angular-test-app/src/assets/fonts.css"
            ],
            "scripts": [
              {
                "input": "node_modules/document-register-element/build/document-register-element.js"
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/angular-test-app/src/environments/environment.ts",
                  "with": "apps/angular-test-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "test:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "test:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "test:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/angular-test-app/tsconfig.app.json",
              "apps/angular-test-app/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/angular-test-app/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/angular-test-app/jest.config.js",
            "tsConfig": "apps/angular-test-app/tsconfig.spec.json",
            "setupFile": "apps/angular-test-app/src/test-setup.ts"
          }
        }
      }
    },
    "test-e2e": {
      "root": "apps/angular-test-app-e2e",
      "sourceRoot": "apps/angular-test-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/angular-test-app-e2e/cypress.json",
            "tsConfig": "apps/angular-test-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "test:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "test:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/angular-test-app-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**",
              "!apps/angular-test-app-e2e/**"
            ]
          }
        }
      }
    },
    "angular-components": {
      "projectType": "library",
      "root": "libs/angular-components",
      "sourceRoot": "libs/angular-components/",
      "prefix": "uxg",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/angular-components/tsconfig.lib.json",
            "project": "libs/angular-components/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/angular-components/tsconfig.lib.json",
              "libs/angular-components/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/angular-components/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/angular-components/jest.config.js",
            "tsConfig": "libs/angular-components/tsconfig.spec.json",
            "setupFile": "libs/angular-components/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "angular-theme": {
      "projectType": "library",
      "root": "themes/angular-theme",
      "sourceRoot": "themes/angular-theme/",
      "prefix": "uxg",
      "architect": {
        "build": {
          "builder": "./dist/tools/devkit:theme-builder",
          "options": {
            "inputPath": "themes/angular-theme/",
            "outputPath": "dist/themes/angular-theme",
            "assets": [
              "themes/angular-theme/lib",
              "themes/angular-theme/assets",
              "themes/angular-theme/*.scss"
            ],
            "sourceMap": true
          },
          "configurations": {
            "production": {
              "outputStyle": "compressed"
            }
          }
        },
        "lint": {
          "builder": "./dist/tools/devkit:theme-linter",
          "options": {
            "project": "themes/angular-theme"
          }
        }
      },
      "schematics": {
        "@ffdc/angular:theme": {
          "styleext": "scss"
        }
      }
    },
    "angular-elements": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/angular-elements",
      "sourceRoot": "apps/angular-elements/src",
      "prefix": "ffdc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          // "builder": "ngx-build-plus:build",
          "options": {
            "outputPath": "dist/apps/angular-elements",
            "index": "apps/angular-elements/src/index.html",
            "main": "apps/angular-elements/src/main.ts",
            "polyfills": "apps/angular-elements/src/polyfills.ts",
            "tsConfig": "apps/angular-elements/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/angular-elements/src/favicon.ico",
              "apps/angular-elements/src/assets"
            ],
            "styles": [
              "apps/angular-elements/src/styles.scss"
            ],
            "scripts": [  
              {
              "input": "node_modules/document-register-element/build/document-register-element.js"
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/angular-elements/src/environments/environment.ts",
                  "with": "apps/angular-elements/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular-elements:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-elements:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-elements:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/angular-elements/tsconfig.app.json",
              "apps/angular-elements/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/angular-elements/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/angular-elements/jest.config.js",
            "tsConfig": "apps/angular-elements/tsconfig.spec.json",
            "setupFile": "apps/angular-elements/src/test-setup.ts"
          }
        }
      }
    },
    "angular-elements-e2e": {
      "root": "apps/angular-elements-e2e",
      "sourceRoot": "apps/angular-elements-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/angular-elements-e2e/cypress.json",
            "tsConfig": "apps/angular-elements-e2e/tsconfig.e2e.json",
            "devServerTarget": "angular-elements:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-elements:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/angular-elements-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/angular-elements-e2e/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "test"
}